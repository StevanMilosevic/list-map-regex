package list;

import java.util.*;

// LinkedLista je u osnovi niz parova (element, pokaziva훾) pri 훾emu pokaziva훾 sadr탑i adresu narednog para

public class LinkedListDemo {

	public static void main(String[] args) {
		List<Integer> kolekcija = new LinkedList<>();
		
		// add() dodaje elemente u listu
		kolekcija.add(31);	// 0
		kolekcija.add(12);	// 1
		kolekcija.add(9);	// 2
		kolekcija.add(64);	// 3
		kolekcija.add(107);	// 4
		kolekcija.add(77);	// 5
		kolekcija.add(359);	// 6
		
		// moze i ovako da se ubaci nov element u kolekciju
		kolekcija.add(7, 83);	// na indeks 7, ubacujemo broj 83
		
		
		

	}

}
